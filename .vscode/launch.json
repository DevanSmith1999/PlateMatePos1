    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Server",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false
        },
        {
            "name": "Apply Migrations",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
              "migrate"
            ],
            "django": true,
            "autoStartBrowser": false
        },      
        {
            "name": "Make Migrations",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
              "makemigrations"
            ],
            "django": true,
            "autoStartBrowser": false
        }
    ]
}